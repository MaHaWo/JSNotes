{
  "name": "jsnotes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest run ./tests/*.test.js",
    "lint": "prettier --check ./src ./tests && eslint ./src ./tests",
    "format": "prettier --write ./src ./tests",
    "prepare": "cd ../.. && husky frontend/JSNotes/.husky"
  },
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "svelte": "^4.2.18",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "eslint": "^9.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "vitest": "^2.0.5"
  }
}
